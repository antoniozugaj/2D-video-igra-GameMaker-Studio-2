        case 1: room_goto(rm_tutorial);
                break;
        case 2: obj_score.new_time=0;
                obj_score.time1=0;
                scr_save();
                break;
        case 3: break;
        case 4: room_goto(rm_store);
                break;
        case 5:game_end();
                break;
